{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "console",
        "PatternLayout": {
          "pattern": "%d{dd/MM/yyyy HH:mm:ss} %-5p %c{1}:%L - %m%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "INFO",
        "AppenderRef": {
          "ref": "console"
        }
      }
    }
  }
}